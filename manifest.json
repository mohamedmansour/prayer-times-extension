{
    "update_url": "http://0.0.0.0",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' https://maps.google.com https://maps.googleapis.com https://maps.gstatic.com; object-src 'self'",
    "background": {
          "page": "background.html",
          "persistent": true
     },
    "browser_action": {
        "default_icon": "img/icon24.png",
        "default_popup": "popup.html"
    },
    "default_locale": "en",
    "description": "Notice: Original deveoper is `Mohamed Mansour'. I only corrected the extension so that it is compatible with Chrome 67+.",
    "icons": {
        "128": "img/icon128.png",
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png"
    },
    "minimum_chrome_version": "6",
    "name": "Prayer Times",
    "short_name": "Prayer Times",
    "options_page": "options.html",
    "permissions": [ "geolocation" , "notifications"],
    "version": "5.0.1"
}
